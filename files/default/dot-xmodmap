! Invert Scrolling for "Normal Things"
pointer = 1 2 3 5 4 6 7 8 9 10 11 12

! Swap CAPSLOCK with Control
! Swap Alt with Super
!
clear lock
clear control
clear mod4
clear mod1
keycode 66 = Control_L
keycode 37 = Hyper_L
keycode 133 = Alt_L
keycode 64 = Super_L
add control = Control_L Control_R
add mod4 = Super_L Super_R
add mod1 = Alt_L Alt_R
add lock = Caps_Lock
